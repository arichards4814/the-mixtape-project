


<div class="row">
  <div class="col-md-4">
  </div>
  <div class="col-md-4">
  <h2 style="text-align: center">Log In:<h2>
  </div>
  <div class="col-md-4">
  </div>
</div>



<div class="row">

<div class="col-md-4">
</div>
<div class="col-md-4">
<div class="form-group">
  <%= form_tag '/logged' do %>
      
    Username: <%= text_field_tag :user_name, "", :class => "form-control" %>
    Password: <%= password_field_tag :password, "",:class => "form-control" %>
  <%= submit_tag "Submit", :class => "btn btn-primary"%>
  
  <% end %>
</div>
</div>

<div class="col-md-4">
</div>

</div>



<% if @error %>
<h1><%= @error %></h1>
<% end %>
